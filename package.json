{
  "name": "@b2cdev/poc-frontend",
  "version": "1.14.0",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently -n NEXT,TSC -c magenta,cyan \"next dev\" \"yarn ts --watch\"",
    "prod": "concurrently -n NEXT,TSC -c magenta,cyan \"NODE_ENV=production next dev\" \"yarn ts --watch\"",
    "debug": "concurrently -n NEXT,TSC -c magenta,cyan \"NODE_OPTIONS='--inspect' next dev\" \"yarn ts --watch\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "test": "next build",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "lint": "next lint",
    "prettify": "prettier --write \"**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix && yarn prettify",
    "storybook": "start-storybook -p 6006",
    "storybook:rebuild": "rm -rf ./public/storybook && build-storybook --output-dir ./public/storybook",
    "sitemap_index:generate": "esrun ./scripts/generate-sitemap-index",
    "robots_file:generate": "esrun ./scripts/generate-robots-file",
    "postbuild": "concurrently \"yarn run sitemap_index:generate\" \"yarn run robots_file:generate\" \"yarn run storybook:rebuild\""
  },
