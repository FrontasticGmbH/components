  "main": "index.js",
  "private": true,
  "packageManager": "yarn@4.4.1",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build && npm run postbuild",
    "build:analyze": "cross-env NODE_OPTIONS='--max-old-space-size=4096' cross-env ANALYZE=true next build",
    "start": "next start",
    "test": "jest -c jest.config.js",
    "test:coverage": "jest -c jest.config.js --coverage",
    "e2e": "yarn playwright test",
    "e2e:ui": "yarn playwright test --ui",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "test:e2e": "playwright test || echo 'E2E tests failed'",
    "lint": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next lint",
    "lint:fix": "cross-env NODE_OPTIONS='--max-old-space-size=4096' yarn run lint --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build --output-dir ./public/storybook",
    "generate:seo": "esrun ./scripts/generate-sitemap-index && esrun ./scripts/generate-robots-file",
    "postbuild": "rimraf ./public/storybook && npm run storybook:build && npm run generate:seo"
  },
